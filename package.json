{
  "name": "single-market-robot-simulator-viz-plotly",
  "version": "2.2.0",
  "description": "middleware for generating charts with plotly from single-market-robot-simulator simulation logs",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel build src/index.js -o build/index.js",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint src/index.js",
    "lint-test": "node_modules/.bin/eslint test/index.js",
    "test": "node_modules/.bin/mocha --compilers js:babel-register --reporter spec --slow 50 --timeout 60000",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register --timeout 60000",
    "go": "npm run clean && npm run lint && npm run lint-test && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drpaulbrewer/single-market-robot-simulator-viz-plotly.git"
  },
  "keywords": [
    "robot-trading",
    "plot",
    "trading-visualization"
  ],
  "author": "drpaulbrewer@eaftc.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drpaulbrewer/single-market-robot-simulator-viz-plotly/issues"
  },
  "homepage": "https://github.com/drpaulbrewer/single-market-robot-simulator-viz-plotly#readme",
  "dependencies": {
    "d3-array": "^0.8.1",
    "stepify-plotly": "^1.0.2",
    "transpluck": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.7",
    "eslint": "^3.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "should": "^8.3.1"
  },
  "jsnext:main": "src/index.js"
}
